generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model KP_Project {
  id                String   @id @default(uuid())
  name              String
  descriptionCard   String
  descriptionDetail String
  image             String
  demoLink          String
  createdAt         DateTime @default(now())
  updatedAt         DateTime @updatedAt
}
